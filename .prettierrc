  "htmlWhitespaceSensitivity": "ignore",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,

  "overrides": [
    {
      "files": "base.html",
      "options": {
